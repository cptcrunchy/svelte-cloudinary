{
  "name": "svelte-cloudinary",
  "version": "2.3.0",
  "contributors": [
    "Willow (GHOST) <ghostdevbusiness@gmail.com> (https://ghostdev.xyz)",
    "Colby Fayock <hello@colbyfayock.com>",
    "Matias Hernandez <hola@matiashernandez.dev>"
  ],
  "license": "MIT",
  "description": "High-performance image and video delivery and uploading at scale in Svelte powered by Cloudinary.",
  "keywords": [
    "svelte",
    "sveltekit",
    "cloudinary",
    "images",
    "videos",
    "upload",
    "optimization",
    "cdn",
    "responsive-images",
    "image-transformation"
  ],
  "scripts": {
    "dev": "node scripts/version.js && svelte-package --watch --input src",
    "build": "node scripts/version.js && svelte-package --input src",
    "prepublishOnly": "node scripts/version.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && publint",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.36.17"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@testing-library/svelte": "5.2.8",
    "@types/node": "^24.1.0",
    "desm": "^1.3.1",
    "happy-dom": "18.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.36.17",
    "svelte-check": "^4.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "unpic": "^4.1.3",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@cloudinary-util/types": "^1.6.0",
    "@cloudinary-util/url-loader": "^6.0.0",
    "@cloudinary-util/util": "^4.1.0",
    "@unpic/svelte": "1.0.0",
    "defu": "^6.1.4",
    "esm-env": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudinary-community/svelte-cloudinary.git"
  },
  "bugs": {
    "url": "https://github.com/cloudinary-community/svelte-cloudinary/issues"
  },
  "homepage": "https://github.com/cloudinary-community/svelte-cloudinary"
}